{
  "name": "domy-lang",
  "version": "1.0.0",
  "description": "Do My Language",
  "author": "Steven Yuan <s.yuan.all@gmail.com> (syall)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syall/domy.git"
  },
  "scripts": {
    "start": "domy example/test.do",
    "debug": "domy --nolazy --inspect-brk=9229 example/test.do",
    "ref": "node --experimental-modules --no-warnings --experimental-json-modules bin/domy.js example/test.do",
    "test": "true"
  },
  "type": "module",
  "preferGlobal": true,
  "bin": {
    "domy": "bin/domy.js"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "eslint": "^6.7.2"
  },
  "keywords": [
    "domy",
    "esoteric",
    "language",
    "tdop",
    "top down operator precedence",
    "lex",
    "parse",
    "repl",
    "cli"
  ],
  "dependencies": {
    "readline-sync": "^1.4.10"
  }
}
